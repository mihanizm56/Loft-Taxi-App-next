{
  "name": "loft_taxi_next",
  "version": "1.0.0",
  "license": "ISC",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11"
  ],
  "scripts": {
    "lint": "lint-staged",
    "dev": "nodemon server.js",
    "build": "next build",
    "start": "cross-env-shell NODE_ENV=production && node server.js",
    "stop": "taskkill /F /IM node.exe",
    "push": "git add . && npm run lint && git cz && git push",
    "server": "cd server && npm start",
    "test:ci": "set CI=true && jest",
    "test": "jest",
    "install:all": "npm i && cd server && npm i && cd .."
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.4.3",
    "@material-ui/pickers": "^3.2.6",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "connected-next-router": "^1.0.2",
    "date-fns": "^2.0.0-beta.5",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^2.0.0",
    "mapbox-gl": "^1.4.1",
    "next": "latest",
    "next-i18next": "^1.2.1",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^2.0.0",
    "react": "^16.0.0",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.0.0",
    "react-input-mask": "^2.0.4",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-batched-actions": "^0.4.1",
    "redux-form": "^8.2.6",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@commitlint/cli": "^8.2.0",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "cz-customizable": "^6.2.0",
    "dotenv": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.19.3",
    "postcss": "^7.0.18",
    "prettier": "^1.18.2",
    "redux-devtools-extension": "^2.13.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  }
}
